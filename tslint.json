{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "interface-name" : [true, "never-prefix"],
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
        "indent": [true, "tabs", 4],
        "array-type": [true, "generic"],
        "arrow-parens": [true, "ban-single-arg-parens"],
        "trailing-comma": [true, {
            "singleline": "never",
            "multiline": { "objects": "never", "arrays": "never", "functions": "never", "typeLiterals": "ignore" }
        }]
    },
    "rulesDirectory": []
}

